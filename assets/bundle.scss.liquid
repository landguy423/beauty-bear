/* FAQ page */
.page-content {
  max-width: 1450px;
  margin: 0 auto;
  position: relative;
  padding: 0 20px;
  font-size: 18px;
  color: #9B9B9B;
  letter-spacing: 1px;
}
.template-faq .sub-heading {
  max-width: 450px;
  margin: 0 auto;
  text-align: center;
  margin-bottom: 50px;
}
@media (max-width: 767px) {
  .template-faq .sub-heading {
  	margin-bottom: 25px;
  }
}

.section-review-carousel {
  background-color: #d9eef6;
  padding: 30px 0 30px;
  margin-top: 30px;
}

.custom-reviews .section__head h2 {
  margin-bottom: 30px;
}
.custom-reviews .review-list {
  margin: 0 0 30px;
}
.review-block {
  padding: 0 15px;
  color: #e85284;
}
.review-block .content {
  padding: 15px;
  background: #eff9fb;
}
.review-block .review-top { margin-bottom: 15px; }
.review-block .avatar {
  float: left;
  padding-right: 15px;
}
.review-block .review-center { float: left; }

.review-block p { color: #e85284; }
.review-block .view-more {
  font-weight: 600;
  text-decoration: none;
}
.review-block h4 {
  font-family: Oswald, Helvetica, sans-serif;
  font-style: normal;
  font-weight: bold;
  color: #e85284;
  font-size: 23px;
  line-height: normal;
  margin-bottom: 0;
}
.review-block .time, .review-block .review-stars {
  color: #97d3de;
}
.review-block .review-stars {
  float: right;
  font-size: 20px;
  padding-top: 8px;
}
@media (min-width: 851px) {
  .section-review-carousel {
    padding: 68px 0 40px;
    margin-top: 68px;
  }
  .custom-reviews .section__head h2 {
    margin-bottom: 60px;
  }
  .custom-reviews .review-list {
    display: flex;
    margin: 0 0 60px;
  }
  .review-block {
    -webkit-box-flex: 0;
    -ms-flex: 0 0 33.33%;
    flex: 0 0 33.33%;
    max-width: 33.33%;
  }
}
@media (max-width: 850px) {
  .section-review-carousel .page-content {
  	padding: 0;
  }
  .review-block {
    margin-bottom: 15px;
  }
}
