.faq-c {
  padding: 1em 0 0 1em;
  border-bottom: 1px solid #96d3df;
  .faq-q {
    cursor: pointer;
    font-size: 23px;
    text-transform: uppercase;
    font-family: Oswald, Helvetica, sans-serif;
    font-weight: 600;
    color: #e85284;
    letter-spacing: -1px;
    padding-bottom: 0.5em;
    position: relative;
    padding-right: 40px;
    .faq-t {
      font-size: 16px;
      padding-right: 1em;
      -webkit-transition: all 200ms;
      -moz-transition: all 200ms;
      transition: all 200ms;
      color: #e85284;
      padding-top: 10px;
      position: absolute;
      top: 0;
      right: 0;
    }
    .fa-plus, .fa-minus {
      &:before {
        width: 14px;
        max-width: 14px;
        display: block;
        left: 50%;
        top: 50%;
        position: absolute;
        transform: translate(-50%,-50%);
      }
    }
  }
  .faq-a {
    padding-bottom: 1em;
    display: none;
    letter-spacing: 0;
    p {
      color: #9a9a9a;
      max-width: 960px;
    }
  }
  &:first-child {
    border-top: 1px solid #96d3df;
  }
  &.active {
    .faq-a {
      display: block;
    }
  }
}
