/* ------------------------------------------------------------ *\
List Products
\* ------------------------------------------------------------ */
.list-products {
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  margin: 0 -15px;
  .product {
    -webkit-box-flex: 0;
    -ms-flex: 0 0 33.33%;
    flex: 0 0 33.33%;
    max-width: 33.33%;
    padding: 0 15px;
    position: relative;
    .product__image {
      position: relative;
      margin-bottom: 30px;
      a {
        text-align: center;
        display: block;
        img {
          position: relative;
          z-index: 1;
        }
        &:hover {
          .product__foreground {
            bottom: 0;
            right: 0;
            top: 10px;
            left: 10px;
          }
        }
        .product__foreground {
          position: absolute;
          top: 0;
          right: 0;
          bottom: 0;
          left: 0;
          background-color: #d9eef6;
          z-index: 1;
          bottom: 10px;
          right: 10px;
          -webkit-transition: left 0.4s, right 0.4s, top 0.4s, bottom 0.4s;
          -o-transition: left 0.4s, right 0.4s, top 0.4s, bottom 0.4s;
          transition: left 0.4s, right 0.4s, top 0.4s, bottom 0.4s;
        }
        .product__background {
          position: absolute;
          top: 0;
          right: 0;
          bottom: 0;
          left: 0;
          background-color: #fadae6;
          left: 10px;
          top: 10px;
        }
      }
    }
    .product__title {
      padding-left: 10px;
      a {
        text-decoration: none;
        color: #e85284;
        -webkit-transition: color 0.4s;
        -o-transition: color 0.4s;
        transition: color 0.4s;
        h3 {
          font-family: Oswald, Helvetica, sans-serif;
          font-style: normal;
          font-weight: 600;
          line-height: normal;
          font-size: 28px;
          text-transform: uppercase;
          margin-bottom: 0;
          line-height: 1;
        }
        &:hover {
          color: #d9eef6;
        }
      }
    }
    .product__price {
      padding-left: 10px;
      margin-bottom: 15px;
      h4 {
        font-family: Oswald;
        font-style: normal;
        font-weight: 600;
        line-height: normal;
        font-size: 28px;
        text-transform: uppercase;
        color: #96d3df;
      }
    }
    .product__description {
      padding-left: 10px;
      font-style: normal;
      font-weight: normal;
      line-height: 1;
      font-size: 22px;
      color: #707070;
      margin-bottom: 30px;
      p {
        color: #9a9a9a;
        letter-spacing: 0.8px;
        line-height: 1.3;
      }
    }
    .product__actions {
      padding-left: 10px;
    }
  }
  @media (max-width: 767px) {
    display: block;
    margin-bottom: -30px;
    .product {
      max-width: 100%;
      margin-bottom: 30px;
      .product__foreground {
        bottom: 0;
        right: 0;
      }
      .product__background {
        display: none;
      }
      .product__title,
      .product__price,
      .product__description,
      .product__actions {
        padding-left: 0;
      }
      .product__actions {
        .btn {
          width: 100%;
        }
      }
    }
  }
}

