[class^='list-'] {
  list-style: none outside none;
}
.section__head {
  h2 {
    font-family: Oswald, Helvetica, sans-serif;
    font-style: normal;
    font-weight: bold;
    line-height: normal;
    font-size: 32px;
    text-align: center;
    text-transform: uppercase;
    color: #e85284;
