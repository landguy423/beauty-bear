.page-cart {
  .page-title {
    h1 {
      font-size: 52px!important;
    }
  }
}
.page-delivery, .page-privacy, .page-contact, .page-terms, .page-reviews {
  .page-content {
  	max-width: 946px;
    .page-title h1 {
      object-fit: contain;
      font-family: Oswald, Helvetica, sans-serif;
      font-size: 52px;
      font-weight: bold;
      font-style: normal;
      font-stretch: normal;
      text-align: center;
      line-height: 1.5;
      letter-spacing: normal;
      color: #e85284;
      margin-bottom: 0;
    }
    .sub-heading {
      width: 597px;
      margin: 3px auto;
      object-fit: contain;
      font-family: Metric;
      font-size: 22px;
      font-weight: normal;
      font-style: normal;
      font-stretch: normal;
      line-height: 1.09;
      letter-spacing: normal;
      text-align: center;
      color: #e85284;
    }
    @media (max-width: 768px) {
      .page-title h1 {
      	font-size: 27px;
        line-height: 1.37;
      }
      .sub-heading {
      	width:80%;
        font-size: 18px;
        line-height: 1.11;
      }
    }
  }

}

.page-delivery {
  .delivery {
  	margin-top: -50px;
    padding-bottom: 80px;
    background-color: #fdf5f9;

    .page-content {
      margin-top: 50px;     
    }
    @media (max-width: 768px) {
      padding-bottom: 45px;
      .page-content {
        .sub-heading {
          width: 95%;
          margin-top: 15px;
        }
      }
    }
  }
  .section-delivery {
    display: flex;
    flex-wrap: wrap;
    margin-top: 93px;
    .rectangle {
      flex: 0 0 33%;
      max-width: 33%;
      height: 190px;
      padding: 20px;
      text-align: center;
      font-style: normal;
      font-stretch: normal;
      font-size: 28px;
      font-family: Oswald, Helvetica, sans-serif;
      font-weight: bold;
      line-height: normal;
      letter-spacing: normal;
      color: #e85284;
      background-color: #FFF;
      position: relative;

      span.title {
        display: block;
        margin-top: 10px;
      }
      p {
        object-fit: contain;
        font-family: Metric, Helvetica, sans-serif;
        font-size: 18px;
        font-weight: normal;
        line-height: 1.33;
        letter-spacing: normal;
        text-align: center;
        color: #9b9b9b;
        margin-top: 22px;
      }
      span.price {
        display: block;
        position: absolute;
        bottom: 26px;
        left: 43%;
      }
      &:last-child {
/*         margin-left: 4%; */
      }
    }
    @media (max-width: 768px) {
      margin-top: 38px;
      .rectangle {
      	flex: none;
        max-width: 95%;
        width: 95%;
        height: 150px;
        margin: 0 auto;
        margin-bottom: 25px;
        font-size: 22px;

        span.title {
          margin-top: 0;
        }
        p {
          margin-top: 13px;
        }
        span.price {
          bottom: 20px;
        }

        &:last-child {
          margin: 0 auto;
        }
      }
    }
  }

  .returns {
  	margin-top: 50px;
    .returns-description {
      font-family: Metric;
      font-size: 18px;
      font-weight: normal;
      font-style: normal;
      font-stretch: normal;
      line-height: 1.33;
      letter-spacing: normal;
      color: #9b9b9b;
      margin-top: 50px;
      p {
        margin-bottom: 20px;
        &:last-child {
          margin-bottom: 0;
        }
      }
    }
    @media (max-width: 768px) {
      .page-content {
        .sub-heading {
          width: 95%;
        }
      }
    }
  }

  .section-returns {
    margin-top: 75px;
    margin-bottom: 50px;
    .faq-c {
      padding: 1em 0 0 1em;
      border-bottom: 2px solid #e85284;

      &:first-child {
        border-top: 2px solid #e85284;
      }

      .faq-a {
        ul {
          li {
            list-style: disc;
          }
        }
      }
    }
    @media (max-width: 768px) {
      margin-bottom: 0;
      .faq-c {
      	border-bottom-width: 1px;
        &:first-child {
          border-top-width: 1px;
        }
      }
    }
  }
}

.page-terms {
